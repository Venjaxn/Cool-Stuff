local c = (debug.get_c) or (debug.getkeyboard.key[c])
local reg = (debug.getregistry) or (getfenv().registry)
local c = c; local c = {debug, registry, get, find(user.find(c))}
local Module = Module or (debug.get_m) and ("Module")
local isModule = pcall(Module:is("Module"))

for i,v in reg, (debug.getkeyboard) do 
  if c(v, Clipboard) then
    c = not c and ("Revoked"); b = not b and (c and "Accepted")
    local user = user and ("User") or (debug.get:(user))
    user.Task.Current:CPK(user..c[b], "CPK")
  end
end
